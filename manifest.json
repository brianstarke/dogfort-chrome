{
  "name":"dogfort-chrome",
  "version":"0.1",
  "manifest_version":2,
  "description":"Share images and video with your asshole friends.",
  "icons":{
    "128":"img/icon_128.png"
  },
  "browser_action":{
    "default_icon":"img/icon_128.png",
    "default_title":"dogfort",
    "default_popup":"popup.html"
  },
  "background":{
    "scripts":["jquery.js", "bg.js"]
  },
  "permissions":[
    "contextMenus",
    "tabs",
    "cookies",
    "http://api.dogfort.io/*"
  ]
}